#include "pch.h"
#include <iostream>
using namespace std;
int graph[5][5];
int vest[5] = { 0 };//已经进树的点
int lowcost[5];//树到各点的最小权值
void visit(int v) {
	cout << v;
}
void brim(int v) {
	for (int i = 0; i < 5; i++) {
		lowcost[i] = graph[v][i];
	}
	visit(v);
	vest[v] = 1;
	int min,path;
	
	for (int  k = 0; k < 4; k++)
	{
		
		min = 100;
		for (int B = 0; B < 5; B++)
		{
			if (min > lowcost[B] && vest[B] == 0) {
				min = lowcost[B];
				path = B;
			}
		}
		vest[path] = 1;
		visit(path);
		for (int g = 0; g < 5; g++) {
			if (vest[g] == 0 && graph[path][g] < lowcost[g]) {
				lowcost[g] = graph[path][g];
			}
		}
		

	}

}
int main()
{
	for (int i = 0; i < 5; i++) {
		graph[i][i] = 0;
		
	}
	graph[0][1] = 5;
	graph[0][2] = 1;
	graph[0][3] = 2;
	graph[0][4] = 100;
	graph[1][4] = 4;
	graph[1][0] = 5;
	graph[1][2] = 3;
	graph[1][3] = 100;
	graph[2][0] = 1;
	graph[2][1] = 3;
	graph[2][3] = 6;
	graph[2][4] = 2;
	graph[4][2] = 2;
	graph[4][1] = 4;
	graph[4][3] = 3;
	graph[4][0] = 100;
	graph[3][0] = 2;
	graph[3][2] = 6;
	graph[3][4] = 3;
	graph[3][1] = 100;


	brim(0);

	for (int i = 0; i < 5; i++) {
		cout << lowcost[i];
	}

}
